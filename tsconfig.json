{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "CommonJS",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": false,
    "resolveJsonModule": false,
    "esModuleInterop": true,
    "noEmit": false,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": ["**/*"]
}
